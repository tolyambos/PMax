    "@clerk/nextjs": "^4.30.0",
    "@fal-ai/client": "^1.5.0",
    "@langchain/core": "^0.3.45",
    "@langchain/openai": "^0.5.5",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-accordion": "^1.2.11",
