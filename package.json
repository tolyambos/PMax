{
  "name": "pmax",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "FONTCONFIG_PATH=./fonts prisma db push && concurrently --names \"NEXT,PRISMA\" --prefix-colors \"blue,green\" \"CANVAS_FONT_PATH=./fonts next dev\" \"pnpm run db:studio\"",
    "dev:next": "FONTCONFIG_PATH=./fonts CANVAS_FONT_PATH=./fonts prisma db push && next dev",
    "build": "prisma generate && FONTCONFIG_PATH=./fonts CANVAS_FONT_PATH=./fonts next build",
    "build:railway": "pnpm install --frozen-lockfile && prisma generate && FONTCONFIG_PATH=./fonts CANVAS_FONT_PATH=./fonts next build",
    "start": "FONTCONFIG_PATH=./fonts CANVAS_FONT_PATH=./fonts next start",
    "build:production": "prisma generate && prisma db push && FONTCONFIG_PATH=./fonts CANVAS_FONT_PATH=./fonts next build",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node -P tsconfig.json ./scripts/seed.ts",
    "setup-fonts": "node scripts/download-fonts-direct.js",
    "fonts:download": "node scripts/download-fonts-direct.js",
    "fonts:scan": "node scripts/scan-fonts.js",
    "s3:setup": "ts-node -P tsconfig.json ./scripts/setup-s3-buckets.ts"
  },
  "dependencies": {
    "@auth/core": "^0.38.0",
    "@auth/prisma-adapter": "^2.8.0",
    "@aws-sdk/client-s3": "^3.828.0",
